<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="commandFilters" type="System.Configuration.NameValueSectionHandler"/>
  </configSections>
  <startup useLegacyV2RuntimeActivationPolicy="true"><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/></startup>
  <appSettings>
    <add key="XECaptureServer" value="SQL2019"/>
    <add key="XEReplayServer" value="SQL2016"/>
    <add key="LogDirectory" value="C:\Temp\"/>
  </appSettings>
  <commandFilters>
    <add key="Filter1" value="exec sp_reset_connection"/>
    <add key="Filter3" value="sp_unprepare"/>
  </commandFilters>
</configuration>
